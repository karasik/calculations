options{  JDK_VERSION = "1.5";  static = false;}PARSER_BEGIN(EG2)package jjtree;public class EG2{  public static void main(String args [])  {    System.out.println("Reading from standard input...");    System.out.print("Enter an expression like \"1+(2+3)*var;\" :");    EG2 parser = new EG2(System.in);    try    {      SimpleNode n = parser.Start();      n.dump("");      System.out.println("Thank you.");    }    catch (Exception e)    {      System.out.println("Oops.");      System.out.println(e.getMessage());    }  }}PARSER_END(EG2)SKIP :{  " "| "\t"| "\n"| "\r"}// Identifiers.TOKEN : {}// Numbers.TOKEN : {< INTEGER: DIGIT+ >< #DIGIT: [0-9] >}SimpleNode Start() :{}{  Expression() < EOF >  {    return jjtThis;  }}void Expression() :{}{	< INTEGER >}